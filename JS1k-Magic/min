function gr(){var c,a=g.w,b=g.h;b<a?(c=Math.round(b/g.r),g.ry=Math.round(b/c),g.rx=Math.round(a/c)):(c=Math.round(a/g.r),g.rx=Math.round(a/c),g.ry=Math.round(b/c)),g.o=c,g.fb=[];for(var e=g.rx,f=g.ry,h=0;h<e;h++){g.fb[h]=[];for(var i=0;i<f;i++)g.fb[h][i]=["00","00","00"]}i()}function d(){for(var a=g.rx,b=g.ry,d=g.o,e=0,f=0,h=0;h<a;h++){for(var i=0;i<b;i++)c.fillStyle="#"+g.fb[h][i][0]+g.fb[h][i][1]+g.fb[h][i][2],c.fillRect(e,f,d,d),f+=d;e+=d,f=0}}function l(){g.l=setInterval(function(){gr(),d()},lr.framerate)}g={},g.w=d.documentElement.clientWidth,g.h=d.documentElement.clientHeight,g.r=48,g.fr=30,g.fi=[],g.s=16,g.j=g.s+1,g.d=!1,g.d="R",gr(),loop();
