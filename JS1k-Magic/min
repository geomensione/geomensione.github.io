function cP(a,b,c){"W"===c&&(g.fb[a][b]=["FF","FF","FF"])}function drh(){for(var a=Math.floor(Math.random()*(g.rx-g.s)),b=Math.floor(Math.random()*(g.ry-g.s)),c=0;c<g.s;c++)for(var d=0;d<g.s;d++)cP(a+c,b+d,g.sh[d].charAt(c))}function gr(){var c,a=g.w,b=g.h;b<a?(c=Math.round(b/g.r),g.ry=Math.round(b/c),g.rx=Math.round(a/c)):(c=Math.round(a/g.r),g.rx=Math.round(a/c),g.ry=Math.round(b/c)),g.o=c,g.fb=[];for(var e=g.rx,f=g.ry,h=0;h<e;h++){g.fb[h]=[];for(var i=0;i<f;i++)g.fb[h][i]=["00","00","00"]}}function dr(){for(var a=g.rx,b=g.ry,d=g.o,e=0,f=0,h=0;h<a;h++){for(var i=0;i<b;i++)c.fillStyle="#"+g.fb[h][i][0]+g.fb[h][i][1]+g.fb[h][i][2],c.fillRect(e,f,d,d),f+=d;e+=d,f=0}}function l(){g.l=setInterval(function(){gr(),drh(),dr()},g.fr)}var g={};g.w=window.innerWidth,g.h=window.innerHeight,g.r=24,g.s=8,g.fr=100,d.addEventListener("mousedown",function(a){var b=c.getImageData(a.clientX,a.clientY,1,1);0!==b.data[0]&&clearInterval(g.l)}),g.sh=["0WWWWWWW","0W0W0WW0","0WWWWW00","0WW0WW0W","00WWWW0W","WWWWWWWW","00WWWW00","0WW00WW0"],l();
